#!/usr/bin/env python3

import sys
import random

def gcd(a, b):
    while b:
        a, b = b, a % b
    return a

def pollards_rho(n):
    x, y = random.randint(1, n-1), 1
    f = lambda x: (x**2 + 1) % n
    while True:
        x = f(x)
        y = f(f(y))
        d = gcd(abs(x - y), n)
        if d != 1:
            return d

def factorize(n):
    factors = []
    while n != 1:
        if n % 2 == 0:
            factors.append(2)
            n //= 2
        else:
            factor = pollards_rho(n)
            factors.append(factor)
            n //= factor
    return factors

def main(filename):
    with open(filename, 'r') as file:
        for line in file:
            n = int(line.strip())
            factorization = factorize(n)
            print(f"{n}={'*'.join(map(str, factorization))}")

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        print("where <file> is a file containing natural numbers to factor.")
        sys.exit(1)
    filename = sys.argv[1]
    main(filename)
